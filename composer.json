{
    "name": "easybib/can-haz-deploy",
    "description": "A dashboard to check if releases are clean and where they are deployed.",
    "require": {
        "aws/aws-sdk-php": "2.*",
        "doctrine/cache": "@stable"
    },
    "require-dev": {
        "phpmd/phpmd": "2.0.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "till",
            "email": "till@php.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "ImagineEasy\\CanHazDeploy\\": "src/"
        }
    },
    "minimum-stability": "beta",
     "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
